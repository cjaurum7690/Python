    def retranslateUi(self, MainWindow):
        _translate = QtCore.QCoreApplication.translate
        MainWindow.setWindowTitle(_translate("MainWindow", "Lottery Picks"))
        self.radioButton_power.setText(_translate("MainWindow", "PowerBall"))
        self.radioButton_mega.setText(_translate("MainWindow", "MegaBall"))
        self.radioButton_pick4.setText(_translate("MainWindow", "Pick 4"))
        self.radioButton_lucky.setText(_translate("MainWindow", "Cash4Life"))
        self.radioButton_pick3.setText(_translate("MainWindow", "Pick 3"))
        self.label_top_display.setText(_translate("MainWindow", "Your Picks"))
        self.label_bottom_display.setText(_translate("MainWindow", "Last Ball"))
        self.Num_Tickets_Label.setText(_translate("MainWindow", "How Many Tickets?"))
        self.Num_Guesses_Label.setText(_translate("MainWindow", "How Many Guesses?"))
        self.Num_Tickets_Input.setText(_translate("MainWindow", "1"))
        self.Num_Guesses_Input.setText(_translate("MainWindow", "1"))
        self.button_get_numbers.setText(_translate("MainWindow", "Get Numbers"))

    def get_main_picks(self, howManyPicks, numberOfBalls):
        counter = 1
        while counter <= int(self.Num_Tickets_Input.text()):
            main_numbers = []
            for i in range(int(self.Num_Guesses_Input.text())):
                for x in range(howManyPicks):
                    value = random.randint(1, numberOfBalls - 1)
                    main_numbers.append(value)
            main_sort_numbers = sorted(main_numbers)

            main_frequencies = {}
            for item in main_sort_numbers:
                if item in main_frequencies:
                    main_frequencies[item] += 1
                else:
                    main_frequencies[item] = 1

            sorted_values = sorted(main_frequencies.items(), key=operator.itemgetter(1), reverse=True)

            # print the top 5 numbers to the screen
            result = ''
            for num in range(howManyPicks):
                value = sorted_values[num][0]
                result += (str(value) + str('  '))

            if counter == 1:
                self.picksLabelOne.setText(result)
            elif counter == 2:
                self.picksLabelTwo.setText(result)
            elif counter == 3:
                self.picksLabelThree.setText(result)
            elif counter == 4:
                self.picksLabelFour.setText(result)
            elif counter == 5:
                self.picksLabelFive.setText(result)

            counter += 1

    def get_lucky_picks(self, lastBall):
        last_ball_counter = 1
        while last_ball_counter <= int(self.Num_Tickets_Input.text()):
            lucky_number = []

            for i in range(int(self.Num_Guesses_Input.text())):
                value = random.randint(1, lastBall)
                lucky_number.append(value)
            sort_lucky_numbers = sorted(lucky_number)
            lucky_frequencies = {}
            for item in sort_lucky_numbers:
                if item in lucky_frequencies:
                    lucky_frequencies[item] += 1
                else:
                    lucky_frequencies[item] = 1

            lucky_sorted_values = sorted(lucky_frequencies.items(), key=operator.itemgetter(1), reverse=True)
            lucky_result = lucky_sorted_values[0][0]

            if last_ball_counter == 1:
                self.lastBallOne.setText(str(lucky_result))
            elif last_ball_counter == 2:
                self.lastBallTwo.setText(str(lucky_result))
            elif last_ball_counter == 3:
                self.lastBallThree.setText(str(lucky_result))
            elif last_ball_counter == 4:
                self.lastBallFour.setText(str(lucky_result))
            elif last_ball_counter == 5:
                self.lastBallFive.setText(str(lucky_result))

            last_ball_counter += 1

    def send_the_numbers(self):
        if self.radioButton_power.isChecked():
            howManyPicks = 5
            numberOfBalls = 69
            lastBall = 26
            self.label_top_display.setText('PowerBall')
            self.get_main_picks(howManyPicks, numberOfBalls)
            self.get_lucky_picks(lastBall)
        elif self.radioButton_mega.isChecked():
            howManyPicks = 5
            numberOfBalls = 70
            lastBall = 25
            self.label_top_display.setText('MegaMillions')
            self.get_main_picks(howManyPicks, numberOfBalls)
            self.get_lucky_picks(lastBall)
        elif self.radioButton_lucky.isChecked():
            howManyPicks = 5
            numberOfBalls = 60
            lastBall = 4
            self.label_top_display.setText('Cash4Life')
            self.get_main_picks(howManyPicks, numberOfBalls)
            self.get_lucky_picks(lastBall)
        elif self.radioButton_pick4.isChecked():
            howManyPicks = 4
            numberOfBalls = 10
            self.label_top_display.setText('Pick 4')
            self.get_main_picks(howManyPicks, numberOfBalls)
        elif self.radioButton_pick3.isChecked():
            howManyPicks = 3
            numberOfBalls = 10
            self.label_top_display.setText('Pick 3')
            self.get_main_picks(howManyPicks, numberOfBalls)
        else:
            self.no_lotto_selection()

    def start_lotto(self):
        self.lastBallOne.setText('')
        self.lastBallTwo.setText('')
        self.lastBallThree.setText('')
        self.lastBallFour.setText('')
        self.lastBallFive.setText('')
        self.picksLabelOne.setText('')
        self.picksLabelTwo.setText('')
        self.picksLabelThree.setText('')
        self.picksLabelFour.setText('')
        self.picksLabelFive.setText('')

        if int(self.Num_Tickets_Input.text()) < 1 or int(self.Num_Tickets_Input.text()) > 5:
            self.num_tickets_messagebox()
        elif int(self.Num_Guesses_Input.text()) < 1:
            self.num_guesses_messagebox()
        else:
            self.send_the_numbers()

    def no_lotto_selection(self):
        lotto_msg = QMessageBox()
        lotto_msg.setIcon(QMessageBox.Information)

        # setting message for Message Box
        lotto_msg.setText("You must select a Lottery Type.")

        # setting Message box window title
        lotto_msg.setWindowTitle("Lottery Selection")

        # declaring buttons on Message Box
        lotto_msg.setStandardButtons(QMessageBox.Ok | QMessageBox.Cancel)

        # start the app
        retval = lotto_msg.exec_()

    def num_tickets_messagebox(self):
        msg = QMessageBox()
        msg.setIcon(QMessageBox.Information)

        # setting message for Message Box
        msg.setText("Your Number of Tickets must be between 1 and 5.")

        # setting Message box window title
        msg.setWindowTitle("Number of Tickets")

        # declaring buttons on Message Box
        msg.setStandardButtons(QMessageBox.Ok | QMessageBox.Cancel)

        # start the app
        retval = msg.exec_()

    def num_guesses_messagebox(self):
        msg = QMessageBox()
        msg.setIcon(QMessageBox.Information)

        # setting message for Message Box
        msg.setText("The Number of Guesses must be at least 1.")

        # setting Message box window title
        msg.setWindowTitle("Number of Guesses")

        # declaring buttons on Message Box
        msg.setStandardButtons(QMessageBox.Ok | QMessageBox.Cancel)

        # start the app
        retval = msg.exec_()


