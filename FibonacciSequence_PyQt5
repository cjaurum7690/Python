        _translate = QtCore.QCoreApplication.translate
        Fibonacci_Main.setWindowTitle(_translate("Fibonacci_Main", "Fibonacci Sequence"))
        self.pushButton.setText(_translate("Fibonacci_Main", "Get Sequence"))
        self.instruction_label.setText(_translate("Fibonacci_Main",
                                                  "<html><head/><body><p><span style=\" color:#ffff00;\">How many numbers in the Fibonacci Sequence would you like to see?</span></p></body></html>"))


    def fib(self):
        c = int(self.how_many_input.text())
        fib_list = []
        num1 = 0
        num2 = 1

        if c == 0:
            return 'Value must be greater than 0.'
        elif c == 1:
            fib_list = [num1]
        elif c == 2:
            fib_list = [num1, num2]
        else:
            fib_list = [num1, num2]
            for num in range(c - 2):
                num3 = num1 + num2
                fib_list.append(num3)
                num1 = num2
                num2 = num3
        self.results_label.setText(str(''))
        result = ''
        for i in fib_list:
            result += (str(i) + str('  '))
        self.results_label.setText(result)
        self.how_many_input.setText('')

